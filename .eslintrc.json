{
	"root": true,
	"extends": ["eslint:recommended", "plugin:prettier/recommended"],
	"plugins": ["prettier"],
	"parserOptions": {
		"ecmaVersion": 2015,
		"sourceType": "module"
	},
	"env": {
		"es6": true,
		"node": true,
		"mocha": true
	},
	"globals": {
		"expect": true
	},
	"rules": {
		"prettier/prettier": "error",
		"no-prototype-builtins": "off"
	}
}
